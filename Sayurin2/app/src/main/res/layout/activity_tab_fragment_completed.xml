<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".TabFragmentCompleted">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#eae7e7"
        android:orientation="vertical">

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="45dp"

            android:background="#ffffff"
            android:padding="10dp">

            <TextView
                android:id="@+id/textView1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginStart="10dp"
                android:text="History"
                android:textColor="#ff8200"
                android:textSize="20sp" />
        </FrameLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="110dp"
            android:layout_marginTop="2dp"
            android:background="#ffffff"
            android:padding="10dp">

            <ImageView
                android:id="@+id/imageView"
                android:layout_width="77dp"
                android:layout_height="77dp"
                android:layout_marginLeft="10dp"
                android:contentDescription="profile image"
                android:src="@mipmap/ic_launcher_round" />

            <TextView
                android:id="@+id/textView_barang"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@id/imageView"
                android:layout_marginLeft="5dp"
                android:layout_toLeftOf="@id/textView_kirim"
                android:text="Barang :"
                android:layout_marginTop="5dp"
                android:textColor="#5faf50"
                android:textSize="14sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textView_barang1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@id/imageView"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="2dp"
                android:layout_toLeftOf="@id/textView_tanggal"
                android:layout_below="@id/textView_barang"
                android:text="Bayam (2)"
                android:textColor="#5faf50"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/textView_barang2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@id/imageView"
                android:layout_marginLeft="5dp"
                android:layout_toLeftOf="@id/textView_jam"
                android:layout_below="@id/textView_barang1"
                android:layout_marginTop="2dp"
                android:text="Bawang (1)"
                android:textColor="#5faf50"
                android:textSize="14sp"
                />
            <TextView
                android:id="@+id/textViewnote1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@id/imageView"
                android:layout_marginLeft="5dp"
                android:layout_below="@id/textView_barang2"
                android:text="Kangkung (5)"
                android:layout_marginTop="2dp"
                android:textColor="#5faf50"
                android:textSize="14sp"
                />

            <TextView
                android:id="@+id/textView_kirim"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_marginTop="5dp"
                android:layout_weight="1"
                android:layout_marginRight="10dp"
                android:text="Dikirim :"
                android:textAlignment="center"
                android:textColor="#ff8200"
                android:textSize="14sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textView_tanggal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_weight="1"
                android:layout_below="@id/textView_kirim"
                android:text="18 04 2018"
                android:layout_marginRight="10dp"
                android:layout_marginTop="2dp"
                android:textAlignment="center"
                android:textColor="#ff8200"
                android:textSize="14sp"
                />

            <TextView
                android:id="@+id/textView_jam"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_weight="1"
                android:layout_below="@id/textView_tanggal"
                android:text="08.00 WIB"
                android:layout_marginTop="2dp"
                android:textAlignment="center"
                android:textColor="#ff8200"
                android:layout_marginRight="10dp"
                android:textSize="14sp"
                />



        </RelativeLayout>
    </LinearLayout>

</android.support.constraint.ConstraintLayout>